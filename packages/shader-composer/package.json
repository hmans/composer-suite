{
  "name": "shader-composer",
  "author": {
    "name": "Hendrik Mans",
    "email": "hendrik@mans.de",
    "url": "https://hmans.co"
  },
  "description": "Node-based Shader Composition!",
  "version": "0.3.4",
  "main": "dist/shader-composer.cjs.js",
  "module": "dist/shader-composer.esm.js",
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "fun.ts",
      "stdlib/artistic.ts",
      "stdlib/logic.ts",
      "stdlib/masters.ts",
      "stdlib/math.ts",
      "stdlib/rotation.ts",
      "stdlib/scene.ts",
      "stdlib/spaces.ts",
      "stdlib/textures.ts",
      "stdlib/values.ts",
      "stdlib/variables.ts",
      "stdlib/vectors.ts"
    ]
  },
  "files": [
    "stdlib/**",
    "fun/**",
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "three": "^0.143.0"
  },
  "peerDependencies": {
    "three": ">=0.141.0"
  },
  "dependencies": {
    "fp-ts": "^2.12.3"
  }
}
