{
  "name": "shader-composer",
  "author": {
    "name": "Hendrik Mans",
    "email": "hendrik@mans.de",
    "url": "https://hmans.co"
  },
  "description": "Node-based Shader Composition!",
  "version": "0.4.6",
  "main": "dist/shader-composer.cjs.js",
  "module": "dist/shader-composer.esm.js",
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "fun.ts"
    ]
  },
  "files": [
    "fun/**",
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "preconstruct build",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ]
    ]
  },
  "peerDependencies": {
    "three": ">=0.141.0"
  },
  "dependencies": {
    "fp-ts": "^2.12.3"
  }
}
